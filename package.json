{
  "name": "u8-mqtt-packet",
  "description": "MQTT packet codec using Uint8Array -- suitable for use in the browser, nodejs, and deno.",
  "version": "0.0.5",
  "license": "BSD-2-Clause",
  "author": "Shane Holloway <shane.holloway@ieee.org>",
  "homepage": "https://github.com/shanewholloway/js-u8-mqtt-packet#readme",
  "bugs": {
    "url": "https://github.com/shanewholloway/js-u8-mqtt-packet/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/shanewholloway/js-u8-mqtt-packet.git"
  },
  "files": [
    "cjs/",
    "code/",
    "esm/",
    "umd/"
  ],
  "type": "module",
  "main": "./cjs/index.cjs",
  "module": "./esm/index.mjs",
  "browser": "./umd/index.min.js",
  "exports": {
    "./": "./",
    ".": {
      "import": "./esm/index.mjs",
      "require": "./cjs/index.cjs"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^7.1.3",
    "qsrv": "^0.1.5",
    "rollup": "^2.7.3",
    "rollup-plugin-dgnotify": "^0.1.5",
    "rollup-plugin-jsy": "^1.7.1",
    "rollup-plugin-terser": "^5.3.0",
    "u8-mqtt-packet": "file:."
  },
  "scripts": {
    "start": "qsrv -- *.html esm/",
    "clean": "rm -rf ./cjs/* ./esm/* ./umd/*",
    "build": "rollup --config",
    "watch": "npm -s run build -- --watch",
    "pretest": "npm -s run build",
    "test": "cd test; npm -s run test",
    "test:install": "cd test; npm -s install",
    "test:one": "cd test; npm -s run test:one",
    "test:watch": "cd test; npm -s run test:watch"
  }
}
