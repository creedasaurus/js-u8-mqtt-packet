import {mqtt_type_reader, bind_reason_lookup} from './_utils.jsy'

export const _pubxxx_reason_ = bind_reason_lookup @#
  @[] 0x00, 'Success'
  @[] 0x92, 'Packet Identifier not found'


export {_mqtt_decode_pubxxx as mqtt_decode_pubrec}

export function _mqtt_decode_pubxxx(pkt) ::
  const rdr = new mqtt_type_reader(pkt.u8_body, 0)

  pkt.packet_id = rdr.u16()
  pkt.reason = rdr.u8_reason(_pubxxx_reason_)
  if 5 <= pkt.mqtt_level ::
    pkt.props = rdr.props()
  return pkt

