import {mqtt_encode_by_type} from './_encode_all.jsy'

export const _mqtt_bind_encode = enc_by_type =>
  @\ mqtt_level ::
    mqtt_level = +mqtt_level || mqtt_level.mqtt_level
    return @\ type, pkt =>
      enc_by_type[type] @ mqtt_level, pkt


export const mqtt_encode_session =
  _mqtt_bind_encode @
    mqtt_encode_by_type

