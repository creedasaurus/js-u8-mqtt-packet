import {mqtt_type_writer} from './_utils.jsy'

export function mqtt_encode_unsuback(pkt) ::
  const wrt = new mqtt_type_writer()

  wrt.u16(pkt.packet_id)
  if 5 <= pkt.mqtt_level ::
    wrt.props(pkt.props)

  for const ans of pkt.unsub_answers ::
    wrt.u8_reason(ans)

  return wrt.as_pkt(0xb0)

